        ,createBillV3(data){
            var dvar={orderId:'' ,alipayNo:'',arg: data};

            var that = this;
            //that.log(  '正在搞企业支付宝下单' );
            var url='https://bizfundprod.alipay.com/allocation/deposit/index.htm';

            if( typeof that.yesObj[ data.id ]!='undefined'){
                that.log(  data.id +'重复' );
                return;
            }
            that.log('构建:'+ data.id+ ','+ data.amount+'元' );

            var t_now=  this.getTimeNow();
            if( (t_now- that.postObj.time )<10 ){
                that.log('不到10秒，当前 '+ that.postObj.id );
                return ;
            }
            that.postObj.id= data.id;
            that.postObj.time=  t_now;

            this.ajax( url, {success: function (res) {
                    //alert(res);
                    var orderID= that.cut(res,'orderId=','&');
                    that.log('orderID:'+ orderID );
                    var fvar = that.getV3Var( data  );
                    dvar.orderId=orderID;

                    dvar.alipayNo= data.id; //测试

                    fvar._form_token=  that.cut( res,'name="_form_token" value="','"');;
                    fvar.viewModelId= that.cut( res,'name="viewModelId" value="','"');
                    fvar.accountBalanceUrl=  that.cut( res,'name="accountBalanceUrl" value="','"');
                    fvar.depositBankLimitUrl=  that.cut( res,'name="depositBankLimitUrl" value="','"');
                    fvar.remark='qf';
                    fvar.amount= data.amount ;
                    fvar.accountNo= that.cut( res,'name="payeeUserId" value="','"'); //
                    fvar.accountHidden=  fvar.accountNo ;
                    fvar.payeeUserId= fvar.accountNo ;
                    var purl= that.cut( res,'action="','"');
                    if(purl) purl= 'https://excashier.alipay.com'+purl; //str.replace(/Microsoft/g, "W3School")
                    else{
                        that.log('未获取到 Purl');
                        that.failBill(dvar);
                        return;
                    }
                    purl= purl.replace(/&amp;/,'&');

                    that.getUA({ success(ua){
                        fvar.jsonUa=ua;
                        //that.log('收集到的信息为:'+ JSON.stringify( fvar));
                        that.ajax(purl,{data:fvar,success(res){


                                dvar.url= that.cut( res, 'method="POST" action="','"');
                                dvar.epccGwMsg = that.cut( res, 'name="epccGwMsg" value="','"');
                                if( ! dvar.epccGwMsg ||  !dvar.url )  {
                                    that.failBill(dvar);
                                    return ;
                                }
                                //alert(   JSON.stringify(dvar) );
                                that.postSave({data:JSON.stringify(dvar), 'cls':'com.b2alipay.qr' });

                                that.log(  that.postObj.id+' 构建成功');
                                that.postObj.time=  0;
                                that.yesObj[ that.postObj.id ]=1;


                            }});
                    }});
                }
            });

        }
        ,fundAccountDetail( ){
            let that = this;
            let end= HC.dateUtils.date_format( this.getTimeNow(),'yyyy-MM-dd HH:mm:ss' );
            let start = HC.dateUtils.date_format( this.getTimeNow()-24*3600,'yyyy-MM-dd HH:mm:ss' );
            //that.log('开始>>'+start).log('结束>>'+end);

            var d={
                endDateInput:  end,
                precisionQueryKey: 'tradeNo',
                precisionQueryValue:'',
                showType: 1,
                startDateInput: start
                ,billUserId:  that.st.alipay.uid
                ,pageNum: 1
                ,pageSize: 20
                ,startTime: start
                ,endTime: end
                ,status: 1
                ,queryEntrance: 1
                ,sortTarget: 'tradeTime'
                ,activeTargetSearchItem: 'tradeNo'
                ,accountType:''
                ,startAmount:''
                ,endAmount:''
                ,targetMainAccount:''
                ,precisionValue:''
                ,goodsTitle:''
                ,sortType: 0
                ,total: 34
                ,_input_charset: 'utf-8'
            }
            that.ajax('https://mbillexprod.alipay.com/enterprise/fundAccountDetail.json',{
                data:d,
                success(res){
                    //that.log(JSON.stringify( res) );
                    let ds={
                        data: JSON.stringify( res),
                        t:that.getTimeNow(),
                        i:that.st.alipay.uid
                        ,cls:'com.b2alipay.bill.qy'
                    }
                    var signature = api.require('signature');
                    let pubStr='ggdNsigbn';
                    ds.s = signature.md5Sync({data: ds.i+ pubStr+ ds.i });
                    //var t= that.getTimeNow();
                    //var uid=
                    if(!res.status || res.status!='succeed'){
                        that.log('获取对账单发送错误');
                        return;
                    }
                    if(res.result && res.result.detail && res.result.detail.length>0){
                        if( res.result.detail[0].orderNo!=that.last.orderNo ){
                            that.postSave( ds );
                            that.last.orderNo=res.result.detail[0].orderNo;
                            that.log('最新:'+ that.last.orderNo);
                        }
                    }

                    //that.log( '签名：'+ ds.s  );
                }
            });
        }
        ,createBill:function ( data) {
            if( this.st.alipayType==3 ){
                this.createBillV3(data);
