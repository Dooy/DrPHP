                    }
                    conf.success(str);
                });
            }else{

                var opt={
                    dataType:'json',//服务器返回json格式数据
                    type:'get',//HTTP请求类型
                    timeout:10000,//超时时间设置为10秒；
                    //headers:{'Content-Type':'application/json'},
